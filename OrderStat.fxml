<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
     fx:controller="application.TotalOrder" spacing="10" alignment="CENTER">
    <!-- PieChart to display the data -->
    <PieChart fx:id="pieChart" prefWidth="400" prefHeight="300" />

    <!-- VBox for additional details -->
    <VBox fx:id="detailsVBox" spacing="5" alignment="CENTER" />
</VBox>
